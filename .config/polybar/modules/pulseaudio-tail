type =custom/script
tail = true
exec = ~/.config/polybar/custom/pulseaudio-tail.sh
click-right = exec pavucontrol &
click-left = ~/.config/polybar/custom/pulseaudio-tail.sh --mute
scroll-up = ~/.config/polybar/custom/pulseaudio-tail.sh --up
scroll-down = ~/.config/polybar/custom/pulseaudio-tail.sh --down